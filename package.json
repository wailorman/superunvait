{
  "name": "superunvait",
  "version": "1.11.0",
  "description": "",
  "main": "index.js",
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "syntax-async-functions",
      "transform-async-to-generator"
    ]
  },
  "scripts": {
    "test:mocha": "node ./node_modules/.bin/mocha --harmony --require test/preconf/preconf-mocha.js --compilers js:babel-register ./test/unit/**/*.js",
    "test:www:unit": "node ./node_modules/.bin/mocha --harmony --require ./public/test/requirements/mocha.js ./public/test/unit/**/*.js",
    "test:www:integration": "node ./node_modules/karma/bin/karma start",
    "serve:webpack": "node ./node_modules/.bin/webpack-dev-server --content-base public/dist/",
    "serve:browser-sync": "node ./node_modules/.bin/browser-sync start --server --files \"public/dist/*\"",
    "build": "node ./node_modules/webpack/bin/webpack.js",
    "build:watch": "node ./node_modules/webpack/bin/webpack.js --watch",
    "start:docker": "NODE_ENV=production npm run migrate && NODE_ENV=production node_modules/forever/bin/forever start -c \"node --harmony\" ./src/app.js && node node_modules/forever/bin/forever --fifo logs 0",
    "start:heroku": "node --harmony ./src/app.js",
    "start": "NODE_ENV=production node --harmony ./src/app.js",
    "start:production:win": "set NODE_ENV=production&& node ./src/app.js",
    "start:dev": "node --harmony ./src/app.js",
    "migrate": "node node_modules/sequelize-cli/bin/sequelize db:migrate",
    "build:test:unit": "node ./node_modules/webpack/bin/webpack.js --config webpack.config-test.js",
    "serve:test:client:integration": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.config-mocha.js --content-base \"./dist/test/integration-client\"",
    "build:crx": "node ./node_modules/webpack/bin/webpack.js && node ./node_modules/gulp/bin/gulp.js default",
    "build:test": "node ./node_modules/webpack/bin/webpack.js --config webpack.config-mocha.js",
    "test:api:integration": "node ./node_modules/mocha/bin/mocha --harmony --require ./test/requirements/mocha.js --colors ./test/integration/**/*.int.js",
    "test:api:unit": "node ./node_modules/mocha/bin/mocha --harmony --require ./test/requirements/mocha.js --colors ./test/unit/**/*.unit.js",
    "publish-extension": "npm run build && gulp crx && gulp zip && node ./bin/publish-extension.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wailorman/superunvait.git"
  },
  "author": "wailorman@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wailorman/superunvait/issues"
  },
  "homepage": "https://github.com/wailorman/superunvait",
  "dependencies": {
    "async": "^1.5.2",
    "babel-cli": "^6.18.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "body-parser": "^1.14.2",
    "cheerio": "^0.22.0",
    "date-fns": "^1.24.0",
    "express": "^4.13.4",
    "forever": "^0.15.1",
    "fs": "0.0.2",
    "fs-promise": "^1.0.0",
    "jquery": "^2.2.0",
    "lodash": "^4.1.0",
    "md5": "^2.1.0",
    "mysql": "^2.10.2",
    "ok.ru": "^1.2.0",
    "path": "^0.12.7",
    "q": "^1.4.1",
    "request": "^2.72.0",
    "sequelize": "https://github.com/wailorman/sequelize/tarball/master",
    "sequelize-cli": "^2.3.1"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.0",
    "babel-node-debug": "^2.0.0",
    "babel-plugin-transform-runtime": "^6.1.18",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.1.18",
    "babel-register": "^6.4.3",
    "babel-runtime": "^6.5.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "gulp": "^3.9.0",
    "gulp-crx-pack": "^1.0.1",
    "gulp-zip": "^3.2.0",
    "jsdom": "^8.1.0",
    "json-loader": "^0.5.4",
    "karma": "^0.13.15",
    "karma-chrome-launcher": "^0.2.2",
    "karma-cli": "^0.1.1",
    "karma-fixture": "^0.2.5",
    "karma-html2js-preprocessor": "^0.1.0",
    "karma-live-preprocessor": "^0.2.2",
    "karma-mocha": "^0.2.1",
    "karma-sinon": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "mocha-loader": "^0.7.1",
    "nock": "^7.2.2",
    "null-loader": "^0.1.1",
    "raw-loader": "^0.5.1",
    "sinon": "^2.0.0-pre",
    "sprintf": "^0.1.5",
    "sqlite3": "^3.1.1",
    "style-loader": "^0.13.0",
    "supertest": "^1.1.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.1",
    "write-file-webpack-plugin": "^3.1.8"
  }
}
