{
  "name": "SUPER UNVAITE",
  "version": "1.1.0",
  "description": "Приглашает в группы!",
  "icons": {
    "19": "19.png",
    "64": "64.png",
    "128": "128.png"
  },
  "permissions": [
    "*://*/*",
    "activeTab",
    "tabs"
  ],
  "manifest_version": 2,
  "content_security_policy": "default-src 'none'; style-src 'self'; script-src 'self';",
  "content_scripts": [
    {
      "matches": [
        "*://ok.ru/online"
      ],
      "js": [
        "content_script.build.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "page_script.build.js"
  ]
}
