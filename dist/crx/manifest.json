{
  "name": "SUPER UNVAITE",
  "version": "1.12.0",
  "description": "Приглашает в группы!",
  "icons": {
    "19": "19.png",
    "64": "64.png",
    "128": "128.png"
  },
  "permissions": [
    "*://ok.ru/*",
    "activeTab",
    "tabs",
    "background"
  ],
  "background": {
    "scripts": [
      "background.build.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_icon": {
      "19": "19.png",
      "38": "38.png"
    },
    "default_title": "IBB Tools",
    "default_popup": "post-hunter/page-action.html"
  },

  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_scripts": [
    {
      "matches": [
        "*://ok.ru/*"
      ],
      "js": [
        "content_script.build.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "page_script.build.js"
  ]
}
